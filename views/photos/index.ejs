<%- include("../partials/header") %>

<h1 class="index-h1 fade-in">All Dem Photos</h1>
<a class="color" href="/">HOME</a> |
<a class="color" href="/photos/new" alt="Enter New Image">ENTER A NEW IMAGE</a> |
<a class="color" href="/users/">USERS</a>
<main class="index">

<div class="users">
<% for(let i = 0; i < users.length; i++){ %>
<%= users[i].user.toUpperCase() %>
<div class="photo">
    <% for(let j = 0; j < users[i].photo.length; j++){ %>
        <a class="color" href="/photos/<%=users[i].photo[j]._id%>">Title: <%=users[i].photo[j].name.toUpperCase()%></a> 
        <img src="<%=users[i].photo[j].image%>" alt="<%= users[i].photo[j].name %>">
    <%}%>
</div>
<%}%>
</div>

</main>


<%- include("../partials/footer") %>